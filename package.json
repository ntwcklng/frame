{
  "name": "frame-cli",
  "version": "0.0.1",
  "description": "FRAME",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && npm run lint && ava --fail-fast --verbose",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:watch": "rimraf lib && babel src --watch --out-dir lib",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix"
  },
  "bin": {
    "frame": "./lib/bin/frame.js"
  },
  "files": [
    "lib",
    "templates"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntwcklng/frame.git"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ],
      "stage-2"
    ]
  },
  "standard": {
    "ignore": [
      "/lib/**",
      "/templates/**"
    ]
  },
  "keywords": [],
  "author": "Marvin Mieth <ntwcklng@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntwcklng/frame/issues"
  },
  "homepage": "https://github.com/ntwcklng/frame#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "boxen": "^0.8.1",
    "chalk": "^1.1.3",
    "copy-template-dir": "^1.3.0",
    "glob": "^7.1.1",
    "inquirer": "^2.0.0",
    "minimist": "^1.2.0",
    "ora": "^0.4.0",
    "rimraf": "^2.5.4",
    "snazzy": "^5.0.0",
    "standard": "^8.6.0",
    "temp": "^0.8.3",
    "update-notifier": "^1.0.3"
  },
  "devDependencies": {
    "ava": "^0.17.0"
  }
}
