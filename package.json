{
  "name": "frame-cli",
  "version": "0.0.5",
  "description": "FRAME - Create a new React/Preact Project from the CLI.",
  "main": "./build/cli.js",
  "scripts": {
    "test": "xo && ava",
    "pretest": "npm run build",
    "prepublish": "npm run build",
    "build": "rimraf build && babel src --out-dir build",
    "dev": "rimraf build && babel src --watch --out-dir build",
    "lint": "xo"
  },
  "bin": {
    "frame": "./build/bin/frame.js"
  },
  "files": [
    "build",
    "templates"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntwcklng/frame.git"
  },
  "engines": {
    "node": ">=6.2.0"
  },
  "ava": {
    "failFast": true,
    "files": "test/*.js"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ],
      "stage-2"
    ]
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "space": 2,
    "ignores": [
      "build/**",
      "templates/**"
    ]
  },
  "keywords": [],
  "author": "Marvin Mieth <ntwcklng@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntwcklng/frame/issues"
  },
  "homepage": "https://github.com/ntwcklng/frame#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "boxen": "^0.8.1",
    "chalk": "^1.1.3",
    "copy-template-dir": "^1.3.0",
    "glob": "^7.1.1",
    "inquirer": "^2.0.0",
    "log-symbols": "^1.0.2",
    "minimist": "^1.2.0",
    "ora": "^0.4.0",
    "rimraf": "^2.5.4",
    "temp": "^0.8.3",
    "update-notifier": "^1.0.3"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "xo": "^0.17.1"
  }
}
